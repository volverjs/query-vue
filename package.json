{
    "name": "@volverjs/query-vue",
    "description": "Repository pattern applied to a pinia store.",
    "author": "8 Wave",
    "license": "MIT",
    "keywords": [
        "repository",
        "pinia"
    ],
    "homepage": "https://github.com/volverjs/query-vue",
    "repository": {
        "type": "git",
        "url": "https://github.com/volverjs/query-vue"
    },
    "bugs": {
        "url": "https://github.com/volverjs/query-vue/issues"
    },
    "version": "0.0.0",
    "engines": {
        "node": ">= 16.x"
    },
    "packageManager": "pnpm@8.7.5",
    "scripts": {
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "type-check": "tsc --noEmit",
        "dev": "vite build --watch",
        "build": "vite build",
        "test": "pnpm run test-vitest",
        "test-vitest": "vitest run",
        "test-vitest-watch": "vitest",
        "credits": "npx @opengovsg/credits-generator"
    },
    "type": "module",
    "sideEffects": false,
    "main": "./dist/index.umd.js",
    "module": "./dist/index.es.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "src",
        "*.d.ts"
    ],
    "dependencies": {
        "@volverjs/data": "2.0.0-beta.3",
        "@vueuse/core": "^10.7.2",
        "pinia": "^2.1.7",
        "vue": "^3.4.18"
    },
    "devDependencies": {
        "@pinia/testing": "^0.1.3",
        "@typescript-eslint/eslint-plugin": "^7.0.1",
        "@vitejs/plugin-vue": "^5.0.3",
        "@vue/eslint-config-typescript": "^12.0.0",
        "@vue/test-utils": "^2.4.4",
        "copy": "^0.3.2",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-vue": "^9.21.1",
        "happy-dom": "^13.3.8",
        "prettier": "^3.2.5",
        "typescript": "^5.3.3",
        "vite": "^5.1.0",
        "vite-plugin-dts": "^3.7.2",
        "vite-plugin-eslint": "^1.8.1",
        "vitest": "^1.2.2",
        "vitest-fetch-mock": "^0.2.2",
        "vue-tsc": "^2.0.2"
    },
    "typesVersions": {
        "*": {
            "*": [
                "dist/index.d.ts"
            ]
        }
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.js"
        },
        "./src/*": "./src/*",
        "./dist/*": "./dist/*"
    }
}