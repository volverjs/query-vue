{
    "name": "@volverjs/query-vue",
    "description": "Repository pattern applied to a pinia store.",
    "author": "24/Consulting",
    "license": "MIT",
    "keywords": [
        "repository",
        "pinia"
    ],
    "homepage": "https://github.com/volverjs/query-vue",
    "repository": {
        "type": "git",
        "url": "https://github.com/volverjs/query-vue"
    },
    "bugs": {
        "url": "https://github.com/volverjs/query-vue/issues"
    },
    "version": "0.0.0",
    "engines": {
        "node": ">= 16.x"
    },
    "packageManager": "pnpm@7.8.0",
    "scripts": {
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "type-check": "tsc --noEmit",
        "build": "vite build && vue-tsc --declaration --emitDeclarationOnly && copy src/**/types.d.ts dist",
        "test": "pnpm run test-vitest",
        "test-vitest": "vitest run",
        "test-vitest-watch": "vitest",
        "credits": "npx @opengovsg/credits-generator"
    },
    "type": "module",
    "sideEffects": false,
    "main": "./dist/index.umd.js",
    "module": "./dist/index.es.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "src",
        "*.d.ts"
    ],
    "dependencies": {
        "@volverjs/data": "1.0.3",
        "@vueuse/core": "^10.1.2",
        "pinia": "^2.0.36",
        "vue": "^3.3.2"
    },
    "devDependencies": {
        "@pinia/testing": "^0.1.0",
        "@typescript-eslint/eslint-plugin": "^5.59.5",
        "@typescript-eslint/parser": "^5.59.5",
        "@vitejs/plugin-vue": "^4.2.3",
        "@vue/eslint-config-typescript": "^11.0.3",
        "@vue/test-utils": "^2.3.2",
        "copy": "^0.3.2",
        "eslint": "^8.40.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-vue": "^9.12.0",
        "happy-dom": "^9.18.3",
        "prettier": "^2.8.8",
        "typescript": "^5.0.4",
        "vite": "^4.3.5",
        "vite-plugin-eslint": "^1.8.1",
        "vitest": "^0.31.0",
        "vitest-fetch-mock": "^0.2.2",
        "vue-tsc": "^1.6.5"
    },
    "typesVersions": {
        "*": {
            "*": [
                "dist/index.d.ts"
            ]
        }
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.js"
        },
        "./src/*": "./src/*",
        "./dist/*": "./dist/*"
    }
}