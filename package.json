{
    "name": "@volverjs/query-vue",
    "type": "module",
    "version": "0.0.0",
    "packageManager": "pnpm@10.4.0",
    "description": "Repository pattern applied to a pinia store.",
    "author": "8 Wave S.r.l.",
    "license": "MIT",
    "homepage": "https://github.com/volverjs/query-vue",
    "repository": {
        "type": "git",
        "url": "https://github.com/volverjs/query-vue"
    },
    "bugs": {
        "url": "https://github.com/volverjs/query-vue/issues"
    },
    "keywords": [
        "repository",
        "pinia"
    ],
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.js"
        },
        "./src/*": "./src/*",
        "./dist/*": "./dist/*"
    },
    "main": "./dist/index.umd.js",
    "module": "./dist/index.es.js",
    "types": "./dist/index.d.ts",
    "typesVersions": {
        "*": {
            "*": [
                "dist/index.d.ts"
            ]
        }
    },
    "files": [
        "*.d.ts",
        "dist",
        "src"
    ],
    "engines": {
        "node": ">= 16.x"
    },
    "scripts": {
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "type-check": "tsc --noEmit",
        "dev": "vite build --watch",
        "build": "vite build",
        "test": "pnpm run test-vitest",
        "test-vitest": "vitest run",
        "test-vitest-watch": "vitest",
        "credits": "npx @opengovsg/credits-generator"
    },
    "peerDependencies": {
        "@volverjs/data": "2.0.0-beta.7",
        "@vueuse/core": "^12.x",
        "pinia": "^3.x",
        "vue": "^3.x"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^4.2.0",
        "@nabla/vite-plugin-eslint": "^2.0.5",
        "@pinia/testing": "^1.0.0",
        "@types/node": "^22.13.4",
        "@vitejs/plugin-vue": "^5.2.1",
        "@vue/test-utils": "^2.4.6",
        "copy": "^0.3.2",
        "eslint": "^9.20.1",
        "happy-dom": "^17.1.0",
        "typescript": "5.7.3",
        "vite": "^6.1.0",
        "vite-plugin-dts": "^4.5.0",
        "vitest": "3.0.6",
        "vitest-fetch-mock": "^0.4.3",
        "vue-tsc": "^2.2.0"
    }
}
