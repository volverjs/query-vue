{
    "name": "@volverjs/store-repository-pinia",
    "description": "Repository pattern applied to a pinia store.",
    "author": "24/Consulting",
    "license": "MIT",
    "keywords": [
        "repository",
        "pinia"
    ],
    "homepage": "https://github.com/volverjs/store-repository-pinia",
    "repository": {
        "type": "git",
        "url": "https://github.com/volverjs/store-repository-pinia"
    },
    "bugs": {
        "url": "https://github.com/volverjs/store-repository-pinia/issues"
    },
    "version": "0.0.0",
    "engines": {
        "node": ">= 16.x"
    },
    "packageManager": "pnpm@7.8.0",
    "scripts": {
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "type-check": "tsc --noEmit",
        "build": "vite build && vue-tsc --declaration --emitDeclarationOnly && copy src/**/types.d.ts dist",
        "test": "vitest",
        "credits": "npx @opengovsg/credits-generator"
    },
    "type": "module",
    "sideEffects": false,
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "src",
        "node",
        "*.d.ts"
    ],
    "dependencies": {
        "@volverjs/data": "^0.0.3",
        "@vueuse/core": "^9.13.0",
        "pinia": "^2.0.33",
        "vue": "^3.2.47"
    },
    "devDependencies": {
        "@pinia/testing": "^0.0.15",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "@vue/test-utils": "^2.3.2",
        "copy": "^0.3.2",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "happy-dom": "^8.9.0",
        "prettier": "^2.8.6",
        "typescript": "^5.0.2",
        "vite": "^4.2.1",
        "vite-plugin-eslint": "^1.8.1",
        "vitest": "^0.29.7",
        "vitest-fetch-mock": "^0.2.2",
        "vue-tsc": "^1.2.0"
    },
    "typesVersions": {
        "*": {
            "*": [
                "dist/index.d.ts"
            ],
            "vue": [
                "dist/vue/index.d.ts"
            ]
        }
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "node": "./node/index.js"
        },
        "./src/*": "./src/*",
        "./dist/*": "./dist/*"
    },
    "optionalDependencies": {
        "vue": "^3.2.47"
    }
}