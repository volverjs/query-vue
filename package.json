{
    "name": "@volverjs/query-vue",
    "description": "Repository pattern applied to a pinia store.",
    "author": "24/Consulting",
    "license": "MIT",
    "keywords": [
        "repository",
        "pinia"
    ],
    "homepage": "https://github.com/volverjs/query-vue",
    "repository": {
        "type": "git",
        "url": "https://github.com/volverjs/query-vue"
    },
    "bugs": {
        "url": "https://github.com/volverjs/query-vue/issues"
    },
    "version": "0.0.0",
    "engines": {
        "node": ">= 16.x"
    },
    "packageManager": "pnpm@7.12.2",
    "scripts": {
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "type-check": "tsc --noEmit",
        "build": "vite build && vue-tsc --declaration --emitDeclarationOnly && copy src/**/types.d.ts dist",
        "test": "pnpm run test-vitest",
        "test-vitest": "vitest run",
        "test-vitest-watch": "vitest",
        "credits": "npx @opengovsg/credits-generator"
    },
    "type": "module",
    "sideEffects": false,
    "main": "./dist/index.umd.js",
    "module": "./dist/index.es.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "src",
        "*.d.ts"
    ],
    "dependencies": {
        "@volverjs/data": "1.0.4",
        "@vueuse/core": "^10.4.1",
        "pinia": "^2.1.6",
        "vue": "^3.3.4"
    },
    "devDependencies": {
        "@pinia/testing": "^0.1.3",
        "@typescript-eslint/eslint-plugin": "^6.6.0",
        "@typescript-eslint/parser": "^6.6.0",
        "@vitejs/plugin-vue": "^4.3.4",
        "@vue/eslint-config-typescript": "^11.0.3",
        "@vue/test-utils": "^2.4.1",
        "copy": "^0.3.2",
        "eslint": "^8.48.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-vue": "^9.17.0",
        "happy-dom": "^11.0.2",
        "prettier": "^3.0.3",
        "typescript": "^5.2.2",
        "vite": "^4.4.9",
        "vite-plugin-eslint": "^1.8.1",
        "vitest": "^0.34.3",
        "vitest-fetch-mock": "^0.2.2",
        "vue-tsc": "^1.8.8"
    },
    "typesVersions": {
        "*": {
            "*": [
                "dist/index.d.ts"
            ]
        }
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.js"
        },
        "./src/*": "./src/*",
        "./dist/*": "./dist/*"
    }
}